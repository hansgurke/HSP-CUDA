#! /usr/bin/gnuplot
#usage: gnuplot -e "inputfile='foo.dat'" 3DJulia2png


if (!exists("inputfilename")) inputfilename='default.dat'
set terminal png 
#postscript enhanced
set output inputfilename.'.png'
set xrange[-1:1]
set yrange[-1:1]
set zrange[-1:1]
set view 45,45
splot inputfilename title inputfilename
